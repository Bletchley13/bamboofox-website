<!-- title -->
<div class="page-header <% if (theme.inverse){ %>page-header-inverse <% } %>">
	 <h1 class="title <% if (theme.inverse){ %>title-inverse <% } %>"><i class="fa fa-user"></i> <%= page.title %></h1>
</div>

<div class="row page">

	<% if (theme.widgets.length) { %>
	<div class="col-md-9">
	<% } else { %>
	<div class="col-md-12">
	<% } %>

<div id="top_search"></div>

<% var now = -1; %>
<% site.data.member.executive.forEach(function(profile){ %>
    <% now += 1; %>
    <% if(now%2 === 0){ %>
    <div class="row space-between">
    <% } %>
    <% if(now === 0){ %>
    <div class="col-md-11 col-md-offset-1 col-sm-11 col-sm-offset-1 col-xs-11 col-xs-offset-1" style="padding:20px 0px 20px 0px;font-size:30px">現任幹部(第二屆)</div>
    <% } %>
    <% if(now === 4){ %>
    <div class="col-md-11 col-md-offset-1 col-sm-11 col-sm-offset-1 col-xs-11 col-xs-offset-1" style="padding:20px 0px 20px 0px;font-size:30px">前任幹部(第一屆)</div>
    <% } %>
    <div class="member-container col-md-4 col-md-offset-1 col-sm-4 col-sm-offset-1 col-xs-4 col-xs-offset-1">
        <h2><%= profile['name'] %></h2>
        <div class="img-shadow">
            <img src="/img/<%= profile['id'] %>.png"/>
        </div>
	<div class="member-word">
	    <%= profile['id'] %>
	</div>
        <div class="member-word">
            <%= profile['post'] %>
        </div>
        <a href="<%= profile['github'] %>" class="github">
            <span class="fa-stack fa-lg">
                <i class="fa fa-circle fa-stack-2x"></i>
                <i class="fa fa-github fa-stack-1x fa-inverse"></i>
            </span>
        </a>
    </div>
    <% if(now%2 === 1){ %>
    </div>
    <% } %>
<% }); %>

<% if(now%2 === 0){ %>
    </div>
<% } %>

</div> <!-- col-md-9 -->

	<% if (theme.widgets.length) { %>
		<%- partial('_partial/sidebar') %>
	<% } %>

</div> <!-- row-fluid -->
